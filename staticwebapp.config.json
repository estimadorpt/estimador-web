{
  "routes": [
    {
      "route": "/_next/static/*",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "route": "/",
      "serve": "/index.html"
    },
    {
      "route": "/forecast",
      "serve": "/forecast/index.html"
    },
    {
      "route": "/articles",
      "serve": "/articles/index.html"
    },
    {
      "route": "/about",
      "serve": "/about/index.html"
    },
    {
      "route": "/methodology",
      "serve": "/methodology/index.html"
    },
    {
      "route": "/*",
      "serve": "/404.html",
      "statusCode": 404
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/data/*", "/_next/*", "/*.{css,scss,js,png,gif,ico,jpg,svg,woff,woff2,json}"]
  },
  "mimeTypes": {
    ".json": "application/json"
  },
  "globalHeaders": {
    "cache-control": "public, max-age=31536000, immutable"
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/404.html"
    }
  }
}