{
  "routes": [
    {
      "route": "/_next/static/*",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "route": "/favicon.ico",
      "serve": "/favicon.ico",
      "headers": {
        "cache-control": "public, max-age=86400"
      }
    },
    {
      "route": "/",
      "serve": "/pt/index.html"
    },
    {
      "route": "/pt",
      "serve": "/pt/index.html"
    },
    {
      "route": "/en",
      "serve": "/en/index.html"
    },
    {
      "route": "/pt/forecast",
      "serve": "/pt/forecast/index.html"
    },
    {
      "route": "/en/forecast",
      "serve": "/en/forecast/index.html"
    },
    {
      "route": "/pt/articles",
      "serve": "/pt/articles/index.html"
    },
    {
      "route": "/en/articles",
      "serve": "/en/articles/index.html"
    },
    {
      "route": "/pt/about",
      "serve": "/pt/about/index.html"
    },
    {
      "route": "/en/about",
      "serve": "/en/about/index.html"
    },
    {
      "route": "/pt/methodology",
      "serve": "/pt/methodology/index.html"
    },
    {
      "route": "/en/methodology",
      "serve": "/en/methodology/index.html"
    },
    {
      "route": "/*",
      "serve": "/404.html",
      "statusCode": 404
    }
  ],
  "navigationFallback": {
    "rewrite": "/pt/index.html",
    "exclude": ["/data/*", "/_next/*", "/*.{css,scss,js,png,gif,ico,jpg,svg,woff,woff2,json,txt}"]
  },
  "mimeTypes": {
    ".json": "application/json",
    ".txt": "text/plain"
  },
  "globalHeaders": {
    "cache-control": "public, max-age=31536000, immutable"
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/404.html"
    }
  }
}